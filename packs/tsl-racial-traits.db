{"name":"Waste Walker","type":"feat","img":"icons/environment/wilderness/cave-entrance-rocky.webp","data":{"description":{"value":"<p>Members of this race are considered to have heat protection 1 at all times, and double their Con score when rolling against heat conditions, dehydration, and forced marches.</p>","chat":"","unidentified":""},"tags":[["Idiréailtea"]],"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":"","autoDeductCharges":false,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"racial","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","disabled":false,"identifiedName":"Waste Walker","spellArea":""},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"In2xLaPZGv8dRTFr":3},"flags":{"core":{"sourceId":"Item.XEPjo2bdgtXd2eCf"}},"_id":"NL4E8LuGIQ1Jrn58"}
