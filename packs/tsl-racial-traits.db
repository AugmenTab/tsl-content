{"name":"Signals in the Waves","type":"feat","img":"icons/magic/water/projectile-water-rings.webp","data":{"description":{"value":"<p>Hammerhead selachii have a special suite of sensory organs that make them expert aquatic trackers. They gain a +4 racial bonus to @Compendium[pf1.skills.TXrxgrB2sQjppZFY]{Survival} when tracking in the water.</p>","chat":"","unidentified":""},"tags":[["Selachii (Hammerhead)"]],"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[{"text":"Hammerhead selachii gain a +4 racial bonus to Survival when tracking in the water.","subTarget":"skill.sur"}],"links":{"children":[],"charges":[]},"tag":"","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"racial","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","disabled":false,"identifiedName":"Signals in the Waves"},"effects":[],