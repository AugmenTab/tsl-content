{"name":"Weightless","type":"feat","img":"icons/magic/air/wind-swirl-pink-purple.webp","data":{"description":{"value":"<p>Members of this race have nearly no weight to them, and as such are subject to heavy winds. Additionally, they take a -4 penalty to any check or saving throw to resist strong winds.</p>","chat":"","unidentified":""},"tags":[["Sylvanshine"]],"activation":{"cost":1,"type":""},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":null,"autoDeductCharges":true,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[{"text":"Members of this race take a -4 penalty to any check or saving throw to resist strong winds.","subTarget":"allSavingThrows"}],"links":{"children":[],"charges":[]},"tag":"","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"racial","associations":{"classes":[]},"showInQuickbar":false,"nonlethal":false,"abilityType":"sp","crOffset":"","disabled":false,"identifiedName":"Weightless"},"effects":[],"folder":null,"sort":0,"permission":{"default"