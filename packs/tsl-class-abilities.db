{"name":"Improved Flurry of Blows","type":"feat","img":"systems/pf1/icons/items/weapons/cestus.png","data":{"description":{"value":"<p>At 11th level, a monk can make an additional attack at his highest base attack bonus whenever he makes a flurry of blows. This stacks with the first attack from this ability and additional attacks from haste and similar effects.</p>","chat":"","unidentified":""},"tags":[],"activation":{"cost":1,"type":"full"},"unchainedAction":{"activation":{"cost":3,"type":"action"}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":"","autoDeductCharges":false,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"classFeat_flurryOfBlowsUc","useCustomTag":true,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"classFeat","associations":{"classes":[["Monk (Unchained)"]]},"showInQuickbar":false,"nonlethal":false,"abilityType":"ex","crOffset":"","disabled":false,"spellArea":"","identifiedName":"Improved Flurry of Blows"},"effects":[],"folder":null,"sort":0,"permission":{"default":0