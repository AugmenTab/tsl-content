{"name":"Practiced Toss (Certification)","type":"feat","img":"icons/sundries/gaming/dice-runed-brown.webp","data":{"description":{"value":"<p>A black thumb with this certification is adept at juggling many items and tasks at a time. She is able to receive an item handed to her from another character, and hand off an item to another character, as a free action.</p>","chat":"","unidentified":""},"tags":[],"activation":{"cost":1,"type":"free"},"unchainedAction":{"activation":{"cost":1,"type":""}},"duration":{"value":null,"units":""},"target":{"value":""},"range":{"value":null,"units":"","maxIncrements":1,"minValue":null,"minUnits":""},"uses":{"value":0,"per":"","autoDeductCharges":true,"autoDeductChargesCost":"1","maxFormula":""},"measureTemplate":{"type":"","size":"","overrideColor":false,"customColor":"","overrideTexture":false,"customTexture":""},"attackName":"","actionType":"","attackBonus":"","critConfirmBonus":"","damage":{"parts":[],"critParts":[],"nonCritParts":[]},"attackParts":[],"formulaicAttacks":{"count":{"formula":""},"bonus":{"formula":""},"label":null},"formula":"","ability":{"attack":null,"damage":null,"damageMult":1,"critRange":20,"critMult":2},"save":{"dc":0,"type":"","description":""},"effectNotes":[],"attackNotes":[],"soundEffect":"","powerAttack":{"multiplier":"","damageBonus":2,"critMultiplier":1},"changes":[],"changeFlags":{"loseDexToAC":false,"noEncumbrance":false,"mediumArmorFullSpeed":false,"heavyArmorFullSpeed":false},"contextNotes":[],"links":{"children":[],"charges":[]},"tag":"","useCustomTag":false,"armorProf":{"value":[],"custom":""},"weaponProf":{"value":[],"custom":""},"flags":{"boolean":[],"dictionary":[]},"scriptCalls":[],"featType":"classFeat","associations":{"classes":[["Black Thumb"]]},"showInQuickbar":false,"nonlethal":false,"abilityType":"none","crOffset":"","disabled":false,"identifiedName":"Practiced Toss (Certification)","spellArea":""},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"In2xLaPZGv8dRTFr":3},"flags